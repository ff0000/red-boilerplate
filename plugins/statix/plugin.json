{
	"name": "statix",
	"description": "Statix is a static website generator.",
	"version": "0.0.5",
	"dependencies": {
		"statix": "~2.0.4",
		"wrench": "~1.5.1"
	},
	"systemDependencies": {
		"node": {
			"version": ">=0.9",
			"install": "brew update && brew install node",
			"upgrade": "brew update && brew upgrade node"
		}
	},
	"config": {
		"replaceVars": false,
		"promptOnStart": true
	}
}
